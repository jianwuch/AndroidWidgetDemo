<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!--圆角布局使用-->
  <declare-styleable name="RoundAngleFrameLayout">
    <attr format="reference|dimension" name="radius" />

    <!--那些角需要圆角-->
    <attr name="type">
      <flag name="top_left" value="0x00000001" />
      <flag name="top_right" value="0x00000010" />
      <flag name="bottom_right" value="0x00000100" />
      <flag name="bottom_left" value="0x00001000" />
    </attr>
  </declare-styleable>
</resources>